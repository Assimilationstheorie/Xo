{
	"name": "moovfun/xo",
	"description": "Xo - Php rest api",
	"type": "project",
	"license": "MIT",
	"require": {
		"php": "^7.4",
		"moovfun/xo-route": "^3.0",
		"moovfun/xo-db": "^3.0",
		"moovfun/xo-mail": "^3.0",
		"moovfun/xo-user": "^3.0"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/moovfun/xo-user"
		}
	],
	"autoload": {
        "classmap": [
			"app/"
		],
        "psr-4": {
            "App\\": "app/"
        }
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"scripts": {
        "post-root-package-install": [
			"@php -r \"echo phpversion();\""
        ]
    }
}